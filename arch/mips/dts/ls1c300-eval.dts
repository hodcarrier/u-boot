/dts-v1/;

#include "loongson32-ls1c300b.dtsi"

/ {
	compatible = "lsmips,ls1c300-soc";
	model = "ls1c300-eval";

	aliases {
		serial0 = &uart2;
	};

	memory@80000000 {
		device_type = "memory";
		reg = <0x80000000 0x4000000>;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};
};

&uart2 {
	status = "okay";
};
